          "rows": 5,
          "autoGrow": true
        },
        {
          "id": "wtwlh",
          "name": "文体未来荟",
          "type": "textarea",
          "rows": 5,
          "autoGrow": true
        },
        {
          "id": "oleck",
          "name": "ole超市",
          "type": "textarea",
          "rows": 5,
          "autoGrow": true
        },
        {
          "id": "hrtwx",
          "name": "华润通",
          "type": "textarea",
          "desc": "华润通会风控出现腾讯滑块，建议手动",
          "rows": 5,
          "autoGrow": true,
          "persistentHint": true
        }
      ],
      "icons": [
        "https://raw.githubusercontent.com/fly818/QX/master/icon/hrt.png",
        "https://raw.githubusercontent.com/fly818/QX/master/icon/hrt.png"
      ],
      "author": "@mumu",
      "icon": "https://raw.githubusercontent.com/fly818/QX/master/icon/hrt.png"
    },
    {
      "id": "qc",
      "name": "雀巢",
      "keys": [
        "NESTLE_TOKEN"
      ],
