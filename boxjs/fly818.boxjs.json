{
  "name": "mumu应用订阅",
  "author": "@mumu",
  "repo": "https://github.com/fly818/QX",
  "icon": "https://avatars.githubusercontent.com/u/34331927?s=400&u=b48c29ecc2c9a54784278d4530a5940e147a13dd&v=4",
  "apps": [
    {
      "id": "CSAIR",
      "name": "南方航空",
      "keys": [
        "nfhk_debug",
        "nfhk_sign_in",
        "nfhk_concurrent",
        "nfhk_shuffle",
        "nfhk_log_prize",
        "nfhk_auto_clean",
        "NFHK_ACCOUNTS",
        "NFHK_DEFAULT_RAFFLEID",
        "NFHK_RAFFLEID",
        "nfhk_expired_keywords",
        "nfhk_expired_codes"
      ],
      "descs_html": [
        "<b>建议使用验证码模式登陆</b>"
      ],
      "settings": [
        {
          "id": "nfhk_debug",
          "name": "开启Debug日志",
          "val": "false",
          "type": "boolean",
          "desc": "在日志中打印详细的请求和响应信息，调试用"
        },
        {
          "id": "nfhk_sign_in",
          "name": "开启每日签到",
          "val": "false",
          "type": "boolean",
          "desc": "按需开启，签到活动上线再打开"
        },
        {
          "id": "nfhk_concurrent",
          "name": "开启并发执行",
          "val": "false",
          "type": "boolean",
          "desc": "多账号同时运行，建议保持关闭（顺序执行）"
        },
        {
          "id": "nfhk_shuffle",
          "name": "开启乱序执行",
          "val": "true",
          "type": "boolean",
          "desc": "每次运行打乱账号顺序，模拟真实行为，防止风控检测固定模式"
        },
        {
          "id": "nfhk_log_prize",
          "name": "打印奖品列表",
          "val": "true",
          "type": "boolean",
          "desc": "在日志中显示获取到的奖品详情"
        },
        {
          "id": "nfhk_auto_clean",
          "name": "自动清理过期活动",
          "val": "true",
          "type": "boolean",
          "desc": "自动移除已结束、奖品抽完的活动ID"
        },
        {
          "id": "NFHK_ACCOUNTS",
          "name": "南方航空账号列表",
          "type": "textarea",
          "desc": "请在此处粘贴您的南方航空账号JSON数组。每个账号应包含ID、token和timestamp。示例：\n[{\"ID\":\"123456789\",\"token\":\"abcdef123456\",\"timestamp\":1717939200000},\n{\"ID\":\"987654321\",\"token\":\"fedcba654321\",\"timestamp\":1717852800000}]",
          "rows": 5,
          "autoGrow": true
        },
        {
          "id": "NFHK_DEFAULT_RAFFLEID",
          "name": "固定抽奖活动ID",
          "type": "textarea",
          "desc": "权益专区里的抽奖活动一般持续一个月，获取ID后保存在这里",
          "rows": 2,
          "autoGrow": true,
          "persistentHint": true
        },
        {
          "id": "NFHK_RAFFLEID",
          "name": "临时抽奖活动ID",
          "type": "textarea",
          "desc": "长期活动自行粘贴到上方",
          "rows": 3,
          "autoGrow": true,
          "persistentHint": true
        },
        {
          "id": "nfhk_expired_keywords",
          "name": "过期活动关键词",
          "val": "[\"已结束\", \"奖品已抽完\", \"不在活动时间\", \"活动未开始\"]",
          "type": "textarea",
          "desc": "自定义识别过期的关键词，支持JSON数组或逗号分隔字符串",
          "rows": 3,
          "autoGrow": true
        },
        {
          "id": "nfhk_expired_codes",
          "name": "过期活动错误码",
          "val": "[1105, \"0130\", \"1105\"]",
          "type": "textarea",
          "desc": "自定义识别过期的错误码，支持JSON数组或逗号分隔字符串",
          "rows": 2,
          "autoGrow": true
        }
      ],
      "icons": [
        "https://raw.githubusercontent.com/fly818/QX/master/icon/nfhk.png",
        "https://raw.githubusercontent.com/fly818/QX/master/icon/nfhk.png"
      ],
      "author": "@mumu",
      "icon": "https://raw.githubusercontent.com/fly818/QX/master/icon/nfhk.png"
    },
