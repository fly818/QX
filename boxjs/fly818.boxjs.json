        }
      ],
      "icons": [
        "https://raw.githubusercontent.com/fly818/QX/refs/heads/master/icon/shyp.png",
        "https://raw.githubusercontent.com/fly818/QX/refs/heads/master/icon/shyp.png"
      ],
      "author": "@mumu",
